{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "jsx": "react-native",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist"
  }
}
